{
	"files.associations": {
		"*.h": "c",
		"ctime": "c",
		"condition_variable": "c",
		"shared_mutex": "c"
	},
	"files.exclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/*/**": true,
		"**/.hg/store/**": true,
		"**/LICENSE": true,
		"**/build": true,
		"**/bin": true
	},
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"editor.tabSize": 3
}